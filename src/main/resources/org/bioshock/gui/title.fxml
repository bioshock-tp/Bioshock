<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.TextFlow?>
<?import java.net.URL?>
<?import javafx.scene.text.Text?>
<VBox alignment="CENTER" spacing="30.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.bioshock.gui.TitleController">
    <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>

    <Group>
        <TextFlow styleClass="paragraph">
            <Text styleClass="buzzSpan" fx:id="buzzLabel"/>
            <Text styleClass="killSpan" fx:id="killLabel"/>
        </TextFlow>
    </Group>
</VBox>